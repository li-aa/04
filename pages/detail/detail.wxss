.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shouye:before {
  content: "\e61a";
}

.icon-gouwuche:before {
  content: "\e634";
}

.icon-kefu:before {
  content: "\e6b4";
}

.bottom-nav {
  width: 100%;
  height: 80rpx;
  display: flex;
  position: fixed;
  left: 0;
  bottom: 0;

}

.bottom-nav .iconfont
{
  font-size:16px;
}

.bottom-nav .nav-icon
{
  background-color:#eeeeee;
  flex: 0.5;
  text-align: center;
}

.bottom-nav .nav-addcart {
  flex: 1;
  line-height: 80rpx;
  text-align: center;
  background-color: orange;
  color: #fff;
}

.bottom-nav .nav-buynow {
  flex: 1;
  line-height: 80rpx;
  text-align: center;
  background-color: #ffcc00;
  color: #fff;

}

.desc-box {
  margin-top: 20rpx;
  width: 100%;
  padding:20rpx;
  margin-bottom: 65rpx;
}

.desc-box .desc-img {
  margin: 0;
}

.desc-box image {
  width: 100%;
}